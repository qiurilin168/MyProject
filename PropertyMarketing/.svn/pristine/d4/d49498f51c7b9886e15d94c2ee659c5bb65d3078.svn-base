public interface HouseDAO extends BaseDAO<String, House> {

    /**
     * 单个楼盘的户型计数
     * @param id
     * @return
     */
    long count(String id);

    /**
     * 单个楼盘的户型
     * @param pager
     * @param id
     * @return
     */
