    /**
     * 单个经销商的户型计数
     * @param id
     * @return
     */
    long countHouse(String id);

    /**
     * 单个经销商的户型
     * @param pager
     * @param id
     * @return
     */
    Pager<House> housePager(Pager<House> pager, String id);
